"\e[1~": beginning-of-line
"\e[4~": end-of-line
"\e[5~": beginning-of-history
"\e[6~": end-of-history
"\e[3~": delete-char
"\e[2~": quoted-insert
"\e[5C": forward-word
"\e[5D": backward-word
"\e\e[C": forward-word
"\e\e[D": backward-word
"\ep": history-search-backward
"\e[A": history-search-backward
"\e[B": history-search-forward
# Space: magic-space
# TAB: menu-complete
set completion-ignore-case On

